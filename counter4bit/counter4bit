S_0x61834634c990 .scope module, "tb_counter4bit" "tb_counter4bit" 2 2;
 .timescale -9 -12;
v0x61834635f5b0_0 .var "clk", 0 0;
v0x61834635f680_0 .net "q", 3 0, v0x61834635f3b0_0;  1 drivers
v0x61834635f750_0 .var "rst", 0 0;
S_0x61834634cb20 .scope module, "dut" "counter4bit" 2 7, 3 1 0, S_0x61834634c990;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "q";
v0x61834633b1f0_0 .net "clk", 0 0, v0x61834635f5b0_0;  1 drivers
v0x61834635f3b0_0 .var "q", 3 0;
v0x61834635f490_0 .net "rst", 0 0, v0x61834635f750_0;  1 drivers
E_0x6183463147b0 .event posedge, v0x61834633b1f0_0;
    .scope S_0x61834634cb20;
T_0 ;
    %wait E_0x6183463147b0;
    %load/vec4 v0x61834635f490_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x61834635f3b0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x61834635f3b0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x61834635f3b0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x61834634c990;
T_1 ;
    %vpi_call 2 12 "$monitor", "@time %gns clk=%b rst=%b q=%0d", $time, v0x61834635f5b0_0, v0x61834635f750_0, v0x61834635f680_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61834635f5b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61834635f750_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61834635f750_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61834635f750_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x61834634c990;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x61834635f5b0_0;
    %inv;
    %store/vec4 v0x61834635f5b0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x61834634c990;
T_3 ;
    %delay 200000, 0;
    %vpi_call 2 23 "$stop" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x61834634c990;
T_4 ;
    %vpi_call 2 27 "$dumpfile", "counter4bit.vcd" {0 0 0};
    %vpi_call 2 28 "$dumpvars", 32'sb00000000000000000000000000000001 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
